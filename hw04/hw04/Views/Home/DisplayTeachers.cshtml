@model List<UniversityMVC.Models.Teacher>

@{
    ViewData["Title"] = "Teachers List";
}

<h2>Teachers List</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.Id</td>
                <td>@teacher.FirstName</td>
                <td>@teacher.LastName</td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Index" class="btn btn-primary">Back to Home</a>
